           value = "$(find necst-tz2019)/node_conf/sis_bias_i_reader.conf" />

  </node>

  <node pkg = "necst-tz2019"
        name = "sis_bias_h2_i_reader"
        type = "relay.py">
    <remap from = '/sis_i_cmd' to = '/sis_hl/i'/>
    <remap from = '/dev/pci3177/rsw0/ch' to = '/dev/pci3177/rsw0/ch2'/>
    <param name = "config_file"
           value = "$(find necst-tz2019)/node_conf/sis_bias_i_reader.conf" />

  </node>

  <node pkg = "necst-tz2019"
        name = "sis_bias_v1_i_reader"
        type = "relay.py">
    <remap from = '/sis_i_cmd' to = '/sis_vu/i'/>
    <remap from = '/dev/pci3177/rsw0/ch' to = '/dev/pci3177/rsw0/ch3'/>
        <param name = "config_file"
               value = "$(find necst-tz2019)/node_conf/sis_bias_i_reader.conf" />

  </node>

  <node pkg = "necst-tz2019"
        name = "sis_bias_v2_i_reader"
        type = "relay.py">
    <remap from = '/sis_i_cmd' to = '/sis_vl/i'/>
    <remap from = '/dev/pci3177/rsw0/ch' to = '/dev/pci3177/rsw0/ch4'/>
        <param name = "config_file"
               value = "$(find necst-tz2019)/node_conf/sis_bias_i_reader.conf" />

  </node>

  <node pkg = "necst-tz2019"
        name = "sis_bias_h1_v_reader"
        type = "relay.py">
    <remap from = '/sis_v_cmd' to = '/sis_hu/v'/>
    <remap from = '/dev/pci3177/rsw0/ch' to = '/dev/pci3177/rsw0/ch5'/>
        <param name = "config_file"
               value = "$(find necst-tz2019)/node_conf/sis_bias_v_reader.conf" />

         </node>

         <node pkg = "necst-tz2019"
               name = "sis_bias_h2_v_reader"
               type = "relay.py">
           <remap from = '/sis_v_cmd' to = '/sis_hl/v'/>
           <remap from = '/dev/pci3177/rsw0/ch' to = '/dev/pci3177/rsw0/ch6'/>
           <param name = "config_file"
                  value = "$(find necst-tz2019)/node_conf/sis_bias_v_reader.conf" />

         </node>

         <node pkg = "necst-tz2019"
               name = "sis_bias_v1_v_reader"
               type = "relay.py">
           <remap from = '/sis_v_cmd' to = '/sis_vu/v'/>
           <remap from = '/dev/pci3177/rsw0/ch' to = '/dev/pci3177/rsw0/ch7'/>
           <param name = "config_file"
                  value = "$(find necst-tz2019)/node_conf/sis_bias_v_reader.conf" />

         </node>

         <node pkg = "necst-tz2019"
               name = "sis_bias_v2_v_reader"
               type = "relay.py">
           <remap from = '/sis_v_cmd' to = '/sis_vl/v'/>
           <remap from = '/dev/pci3177/rsw0/ch' to = '/dev/pci3177/rsw0/ch8'/>
           <param name = "config_file"
                  value = "$(find necst-tz2019)/node_conf/sis_bias_v_reader.conf" />


#loatt_section
  </node>

  <node pkg = "necst-tz2019"
