  <node pkg = "necst-tz2019"
        name = "dewar_temp_reader_ch1"
        type = "relay.py">
    <remap from = '/tz2019/dewar/temp' to = '/tz2019/dewar/temp/4k'/>
    <remap from = '/dev/218/ip_192_168_100_187' to = '/dev/218/ip_192_168_100_187/ch1'/>
