<launch>


#sis_bias_writer_section
  <node pkg = "necst-tz2019"
        name = "sis_bias_writer"
        type = "relay.py">
    <param name = "config_file"
           value = "$(find necst-tz2019)/node_conf/sis_bias_writer.conf" />

  </node>

  <node pkg = "necst-tz2019"
        name = "sis_bias_writer_vgap"
        type = "relay.py">
    <param name = "config_file"
           value = "$(find necst-tz2019)/node_conf/sis_bias_writer_vgap.conf" />

  </node>

  <node pkg = "necst-tz2019"
        name = "sis_bias_h1_vp_writer"
        type = "relay.py">
    <remap from = '/sis_vp_cmd' to = '/sis_hu/vp_cmd'/>
    <remap from = '/dev/pci3346A/rsw0/ch' to = '/dev/pci3346A/rsw0/ch1'/>
    <param name = "config_file"
           value = "$(find necst-tz2019)/node_conf/sis_bias_vp_writer.conf" />

  </node>

  <node pkg = "necst-tz2019"
        name = "sis_bias_h2_vp_writer"
        type = "relay.py">
    <remap from = '/sis_vp_cmd' to = '/sis_hl/vp_cmd'/>
    <remap from = '/dev/pci3346A/rsw0/ch' to = '/dev/pci3346A/rsw0/ch2'/>
    <param name = "config_file"
           value = "$(find necst-tz2019)/node_conf/sis_bias_vp_writer.conf" />

  </node>

  <node pkg = "necst-tz2019"
        name = "sis_bias_v1_vp_writer"
        type = "relay.py">
    <remap from = '/sis_vp_cmd' to = '/sis_vu/vp_cmd'/>
    <remap from = '/dev/pci3346A/rsw0/ch' to = '/dev/pci3346A/rsw0/ch3'/>
    <param name = "config_file"
           value = "$(find necst-tz2019)/node_conf/sis_bias_vp_writer.conf" />

  </node>

  <node pkg = "necst-tz2019"
        name = "sis_bias_v2_vp_writer"
        type = "relay.py">
    <remap from = '/sis_vp_cmd' to = '/sis_vl/vp_cmd'/>
    <remap from = '/dev/pci3346A/rsw0/ch' to = '/dev/pci3346A/rsw0/ch4'/>
    <param name = "config_file"
           value = "$(find necst-tz2019)/node_conf/sis_bias_vp_writer.conf" />

  </node>

  <node pkg = "necst-tz2019"
        name = "sis_bias_h1_i_reader"
        type = "relay.py">
    <remap from = '/sis_i_cmd' to = '/sis_hu/i'/>
    <remap from = '/dev/pci3177/rsw0/ch' to = '/dev/pci3177/rsw0/ch1'/>
