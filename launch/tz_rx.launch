        type = "relay.py">
    <remap from = '/tz2019/loatt/i_cmd' to = '/tz2019/loatt_h/i_cmd'/>
    <param name = "config_file"
           value = "$(find necst-tz2019)/node_conf/loatt_icmd_writer.conf" />

  </node>

   <node pkg = "necst-tz2019"
        name = "loattv_icmd_writer"
        type = "relay.py">
